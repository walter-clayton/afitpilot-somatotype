{
  "name": "somatotype",
  "version": "2.0.0",
  "description": "somatotype app",
  "main": "server.js",
  "scripts": {
    "client-instal": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \" npm run server\" \"npm run client\"",
    "build": "npm install  && cd client && npm install && npm run build",
    "render-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build -- prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/walter-clayton/somatotype.git"
  },
  "author": "Walter Clayton",
  "license": "UNLICENSED",
  "private": "true",
  "bugs": {
    "url": "https://github.com/walter-clayton/somatotype/issues"
  },
  "homepage": "https://github.com/walter-clayton/somatotype#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^7.5.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.7.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
